programa {
  funcao inicio() {
    //declaração de variaveis
    real altura,peso_ideal

    //entrada de dados
    escreva("informe a altura: ")
    escreva(altura)
    leia(altura)

    //processamento
    peso_ideal=(72.7*altura)-58

    //saida de dados
    escreva("peso ideal")
    escreva(peso_ideal)
  }
}
